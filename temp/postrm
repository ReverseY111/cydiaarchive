#!/bin/sh

echo "Moving back files..."
mv /System/Library/CoreServices/SpringBoard.app/J2aAP.plist_SiriPortBak /System/Library/CoreServices/SpringBoard.app/J2aAP.plist
mv /System/Library/CoreServices/SpringBoard.app/J1AP.plist_SiriPortBak /System/Library/CoreServices/SpringBoard.app/J1AP.plist
mv /System/Library/CoreServices/SpringBoard.app/K48AP.plist_SiriPortBak /System/Library/CoreServices/SpringBoard.app/K48AP.plist
mv /System/Library/CoreServices/SpringBoard.app/K94AP.plist_SiriPortBak /System/Library/CoreServices/SpringBoard.app/K94AP.plist
mv /System/Library/CoreServices/SpringBoard.app/K93AP.plist_SiriPortBak /System/Library/CoreServices/SpringBoard.app/K93AP.plist
mv /System/Library/LaunchDaemons/com.apple.SpringBoard.plist_SiriPortBak /System/Library/LaunchDaemons/com.apple.SpringBoard.plist
mv /Applications/Preferences.app/Info.plist_SiriPortBak /Applications/Preferences.app/Info.plist
mv /var/mobile/Library/Preferences/com.apple.assistant.plist_SiriPortBak /var/mobile/Library/Preferences/com.apple.assistant.plist

chmod +x /Applications/Preferences.app/Preferences
chmod +x /System/Library/CoreServices/SpringBoard.app/SpringBoard

chmod 755 -R /System/Library/LaunchDaemons/
chown root:wheel -R /System/Library/LaunchDaemons/

chmod 755 /System/Library/CoreServices/SpringBoard.app/J2aAP.plist
chown root:wheel /System/Library/CoreServices/SpringBoard.app/J2aAP.plist

chmod 755 /System/Library/CoreServices/SpringBoard.app/J1AP.plist
chown root:wheel /System/Library/CoreServices/SpringBoard.app/J1AP.plist

chmod 755 /System/Library/CoreServices/SpringBoard.app/K48AP.plist
chown root:wheel /System/Library/CoreServices/SpringBoard.app/K48AP.plist

chmod 755 /System/Library/CoreServices/SpringBoard.app/K94AP.plist
chown root:wheel /System/Library/CoreServices/SpringBoard.app/K94AP.plist

chmod 755 /System/Library/CoreServices/SpringBoard.app/K93AP.plist
chown root:wheel /System/Library/CoreServices/SpringBoard.app/K93AP.plist

chmod 755 /System/Library/LaunchDaemons/com.apple.SpringBoard.plist
chown root:wheel /System/Library/LaunchDaemons/com.apple.SpringBoard.plist

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/Dutch.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/Dutch.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/en_GB.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/en_GB.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/French.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/French.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/German.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/German.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/Italian.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/Italian.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/Japanese.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/Japanese.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/Spanish.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/Spanish.lproj

chmod 755 -R /System/Library/CoreServices/SpringBoard.app/French.lproj
chown root:wheel -R /System/Library/CoreServices/SpringBoard.app/French.lproj

echo "Done. Reboot your Device. Thanks to gdimait!"

