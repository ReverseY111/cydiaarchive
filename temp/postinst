#!/bin/bash
rm -f /Library/RockExtensions/DynamicLibraries/intelliScreen.dylib 
rm -f /Library/RockExtensions/DynamicLibraries/intelliScreen.plist 
if [ ! -h /Library/MobileSubstrate/DynamicLibraries/intelliScreen.dylib ]; then
    ln -s /Library/Intelliborn/intelliScreen /Library/MobileSubstrate/DynamicLibraries/intelliScreen.dylib 
fi 

